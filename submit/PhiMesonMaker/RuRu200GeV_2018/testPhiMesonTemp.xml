<?xml version="1.0" encoding="utf-8" ?>

<job minFilesPerProcess="20" filesPerHour="2" maxFilesPerProcess="25" fileListSyntax="xrootd" simulateSubmission="false">

  <command>
    <!-- starver &#38;library; -->
    root4star -l -b -q fillPhiMesonTree.C\(\"$FILELIST\",\"$JOBID\",&mode;,&beamType;,&flagME;\)&gt;&amp; ${JOBID}.log
  </command>

  <SandBox installer="ZIP">
    <Package>
      <File>file:&codePath;/macro/PhiMesonMaker/fillPhiMesonTree.C</File>
      <File>file:&codePath;/.sl73_gcc485/</File>
      <File>file:&codePath;/StRoot/</File>
      <File>file:&codePath;/Utility/</File>
    </Package>
  </SandBox>

  <input URL="filelist:&listPath;/pico_xrootd_test.list" />
  <!-- <input URL="catalog:star.bnl.gov?production=P20ic,library=SL20c,trgsetupname=production_isobar_2018,collision=RuRu200,filetype=daq_reco_picoDst,storage!=hpss,filename~st_physics" preferStorage="local" singleCopy="true" nFiles="10024"/> -->

  <!-- <stdout discard="true"/> -->
  <stdout URL="file:&outPath;/Log/$JOBID.out"/>
  <stderr URL="file:&outPath;/Log/$JOBID.err"/>

  <Generator>
    <ReportLocation>./JOBS/report/</ReportLocation>
    <ScriptLocation>./JOBS/csh/</ScriptLocation>
    <ListLocation>./JOBS/list/</ListLocation>
  </Generator>

  <output fromScratch="*.root" toURL="file:&outPath;/OutPut/" />
  <output fromScratch="*.log" toURL="file:&outPath;/Log/" />
</job>

